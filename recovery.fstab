# mount point	fstype    device                                   device 2           flags

#                         /dev/block/bootdevice/by-name/ddr
/aboot          emmc      /dev/block/bootdevice/by-name/aboot                         flags=display="aboot";backup=1
/abootbak       emmc      /dev/block/bootdevice/by-name/abootbak                      flags=subpartitionof=/aboot;backup=1
/boot		emmc      /dev/block/bootdevice/by-name/boot                          flags=display="boot";backup=1
/cache		ext4      /dev/block/bootdevice/by-name/cache
flags=display="cache";backup=1;wipeingui;wipeduringfactoryreset
#                         /dev/block/bootdevice/by-name/fsc
/fsg		emmc      /dev/block/bootdevice/by-name/fsg                           flags=display="fsg";backup=1
#                         /dev/block/bootdevice/by-name/keystore
/misc		emmc      /dev/block/bootdevice/by-name/misc                          flags=display="misc";backup=1
/modem		vfat      /dev/block/bootdevice/by-name/modem                         flags=display="modem";backup=1
/modemst1	emmc      /dev/block/bootdevice/by-name/modemst1                      flags=display="modemst1";backup=1
/modemst2	emmc      /dev/block/bootdevice/by-name/modemst2                      flags=display="modemst2";backup=
/pad		emmc      /dev/block/bootdevice/by-name/pad                           flags=display="pad";backup=1
/persist	ext4      /dev/block/bootdevice/by-name/persist                       flags=display="persist";backup=1
/recovery	emmc      /dev/block/bootdevice/by-name/recovery                      flags=display="recovery";backup=1
#                         /dev/block/bootdevice/by-name/rpm
#                         /dev/block/bootdevice/by-name/rpmbak
#                         /dev/block/bootdevice/by-name/safeKeybox
#                         /dev/block/bootdevice/by-name/sbl1
#                         /dev/block/bootdevice/by-name/sbl1bak
#                         /dev/block/bootdevice/by-name/sdi
/splash		emmc      /dev/block/bootdevice/by-name/splash                        flags=display="splash";backup=1
#                         /dev/block/bootdevice/by-name/ssd
/system		ext4      /dev/block/bootdevice/by-name/system                        flags=display="system";backup=1
#                         /dev/block/bootdevice/by-name/tz
#                         /dev/block/bootdevice/by-name/tzbak
/data		ext4      /dev/block/bootdevice/by-name/userdata                      length=-16384
/external_sd	auto      /dev/block/mmcblk1p1                     /dev/block/mmcblk1 flags=display="External SDcard";storage;wipeingui;removable;backup=0     
